@model IEnumerable<Infraction_2.Models.PostModel>
@{
    ViewBag.Title = "Home Page";
}
<!-- LOGO -->
<div class="container">
    <div class="row">
        <div class="header">
            <div class="logo">
                <img src="~/Content/images/logo.jpg" , alt="logo">
            </div>
        </div>
    </div>
</div>
<!-- END LOGO-->
<!-- TOP NAVIGATION -->
<div class="container-fluid">
    <div class="navigation">
        <div class="row">
            <ul class="topnav">
                <li></li>
                <li><a href="index.html"><i class="fa fa-home"></i>  Home</a></li>
                <li><a href="~/WebForm1.aspx"><i class="fa fa-book"></i> OTC Diagram</a></li>
                <li><a href="articles.html"><i class="fa fa-file-text-o"></i> Articles</a></li>
                <li><a href="faq.html"><i class="fa fa-lightbulb-o"></i> FAQ</a></li>
                <li class="icon">
                    <a href="javascript:void(0);" onclick="myFunction()">&#9776;</a>
                </li>
            </ul>
        </div>
    </div>
</div>
<!-- END TOP NAVIGATION -->
<!-- SEARCH FIELD AREA -->
<div class="searchfield bg-hed-six">
    <div class="container" style="padding-top: 10px; padding-bottom: 10px;">
        <div class="row text-center margin-bottom-20">



            <h1 class="white"> Apria Infraction portal</h1>
            <span class="nested"> Learn about Knowledge sharing </span>
        </div>
        <br>
        <div class="row search-row">
            <input class="search" type="text" placeholder="What do you need help with?">
            <a class="buttonsearch btn btn-info btn-lg" href="search-results.html">Search</a>
        </div>
    </div>
</div>
<!-- END SEARCH FIELD AREA -->

<div class="container featured-area-default padding-30">
    <div class="row">
        <div class="col-md-8 padding-20">
            <!-- LATEST VIDEO TUTORIAL AREA -->
            <div class="row margin-bottom-30">
                <div class="col-md-11">
                    <div class="row">
                        <div class="featured-box">
                            <div class="fb-heading">
                                Tracks List of HCL-Apria
                                <div class="fb-sub-heading">
                                    <b>To Know about Tracks?</b> Click on the desired headings<br>
                                </div>
                            </div>
                            <hr class="style-three">
                            <div class="fat-content-small">
                                <ul>
                                    <li><i class="fa fa-file"></i>@Html.ActionLink("Medical Claims", "MedicalClaims", "Home")</li>
                                    <li><i class="fa fa-file"></i>@Html.ActionLink("Electronic Rejections", "Electronicrejection", "Home")</li>
                                    <li><i class="fa fa-file"></i>@Html.ActionLink("Medicare Collections", "Medicare", "Home")</li>
                                    <li><i class="fa fa-file"></i>@Html.ActionLink("Payor Change", "Payorchange", "Home")</li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- END LATEST VIDEO TUTORIAL AREA -->
            <!-- ARTICLE CATEGORIES SECTION -->
            <div class="row">
                <div class="col-md-12">
                    <div class="grid">
                        <div class="fb-heading">
                            Top Infractions

                        </div>
                    </div>
                    <!-- buttons like Newest,votes,active,closed -->
                    <div class="grid ai-center mb16">
                        <div class="grid--cell fl1 fs-body3"></div>
                        <div class="grid--cell">
                            <div class="grid tabs-filter s-btn-group tt-capitalize">

                                <a class="youarehere is-selected grid--cell s-btn s-btn__muted s-btn__outlined py8 ws-nowrap btn-primary" href="https://stackoverflow.com/?tab=interesting" data-nav-xhref="" title="Questions that may be of interest to you based on your history and tag preference" data-value="interesting" data-shortcut="">
                                    Newest_Infraction
                                </a>
                                <a class="grid--cell s-btn s-btn__muted s-btn__outlined py8 ws-nowrap" href="https://stackoverflow.com/?tab=featured" data-nav-xhref="" title="Questions with an active bounty" data-value="featured" data-shortcut="B">
                                    <span class="bounty-indicator-tab">Votes</span>
                                </a>
                                <a class="grid--cell s-btn s-btn__muted s-btn__outlined py8 ws-nowrap" href="https://stackoverflow.com/?tab=hot" data-nav-xhref="" title="Questions with the most views, answers, and votes over the last few days" data-value="hot" data-shortcut="H">
                                    Active
                                </a>
                                <a class="grid--cell s-btn s-btn__muted s-btn__outlined py8 ws-nowrap" href="https://stackoverflow.com/?tab=week" data-nav-xhref="" title="Questions with the most views, answers, and votes this week" data-value="week" data-shortcut="W">
                                    Closed
                                </a>

                            </div>
                        </div>
                    </div>


                    <div id="qlist-wrapper" class="flush-left">
                        <div id="question-mini-list">
                            <div>
                                <!-- Table Values -->
                                @foreach (var item in Model)
                                {



                                    <div class="question-summary narrow">
                                        <div class="cp">
                                            <div class="votes">
                                                <div class="mini-counts"><span title="0 votes">0</span></div>
                                                <div>votes</div>
                                            </div>
                                            <div class="status unanswered">
                                                <div class="mini-counts"><span title="0 answers">0</span></div>
                                                <div>answers</div>
                                            </div>
                                            <div class="views">
                                                <div class="mini-counts"><span title="2 views">2</span></div>
                                                <div>views</div>
                                            </div>

                                        </div>
                                        <div class="summary">
                                            <h4>
                                                <a>
                                                    @Html.DisplayFor(modelItem => item.Title)
                                                </a>
                                            </h4>

                                            <div class="tags">
                                                <div class="btn btn-info btn-xs">
                                                @Html.DisplayFor(modelItem => item.Tags)
                                                    </div>

                                            </div>
                                        </div>
                                    </div>

                                }


                            </div>
                        </div>
                    </div>









                    <a href="knowledge-base.html" class="btn btn-primary pull-right">
                        <i class="fa fa-align-justify"></i> View All
                    </a>
                </div>



            </div>
        </div>

        <!-- SIDEBAR STUFF -->
        <div class="col-md-4 padding-20">
            <div class="row margin-bottom-30">
                <div class="col-md-12 ">
                    <div class="support-container">
                        <h2 class="support-heading">Need To Post Infraction?</h2>
                        <!-- If you like to post infraction to other Tracks <a href="Home/Infraction1">Click Here</a>. -->
                        If you like to post infraction to other Tracks <a>@Html.ActionLink("Click Here", "AddPost", "Post")</a>
                    </div>
                </div>
            </div>
            <div class="row margin-top-20">
                <div class="col-md-12">
                    <div class="fb-heading-small">
                        Popular Infractions
                    </div>
                    <hr class="style-three">
                    <div class="fat-content-small padding-left-10">
                        <ul>
                            <li><i class="fa fa-file-text-o"></i>@Html.ActionLink("Incorrect Medical Group Attached", "MedicalClaims", "Home")</li>

                            <li> <a href="#"><i class="fa fa-file-text-o"></i> CMN To Be Attached For Primary Medicare</a> </li>
