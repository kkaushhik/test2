                        <a href="#" class="btn btn-default btn-o btn-sm">user</a>
                        <a href="#" class="btn btn-default btn-o btn-sm">FI</a>
                        <a href="#" class="btn btn-default btn-o btn-sm">customer</a>
                    </div>
                </div>
            </div>
            <!-- END POPULAR TAGS (SHOW MAX 20 TAGS) -->
        </div>
        <!-- END SIDEBAR STUFF -->

    </div>

</div>







